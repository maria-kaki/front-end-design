-- cria a pasta git e vira um repositório dentro do terminal
git init

-- acompanha o arquivo criado (muda o U pra A)
git add

-- ve o status da branch
git status

-- commit
git commit -m nome

-- ver histórico
git log

-- cria branch
git branch -m main  

-- liga o repositório em um repositório remoto
git remote add origin https://github.com/maria-kaki/web-dev.git

-- mostra os repositorios remotos q vc tem
git remote -v

-- push
git push -u origin main
